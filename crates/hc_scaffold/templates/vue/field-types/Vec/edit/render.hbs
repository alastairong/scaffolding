<div style="display: flex; flex-direction: column">
  <span>{{title_case field_name}}</span>
  
  <div v-for="(el, i) in this._{{camel_case field_name}}">
    {{> (concat field_type "/" widget "/edit/render") label="" initial_value_variable="el" variable_to_change=(concat "this." (camel_case field_name) "[i]" ) }} }
  </div>

  <mwc-button icon="add" label="Add {{title_case field_name}}" @click="this.{{camel_case field_name}}.push(undefined)"></mwc-button>
</div>
